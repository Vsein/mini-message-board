<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p><a href="/new">Send a new message</a><p>
    <main>
      <% messages.forEach(function(message) { %>
      <div>
        <h2><%= message.user %></h2>
        <p><%= message.text %>
        <p><%= message.added %>
      </div>
      <% }); %>
    </main>
  </body>
</html>
